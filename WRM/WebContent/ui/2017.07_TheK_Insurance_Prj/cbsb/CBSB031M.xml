		<script type="javascript"><![CDATA[
			
	var gsSysDate;			// global 현재일자
	var gUserID; 			// global 사용자 ID
	var gUserEmpName; 	    // global 부서명
	var ib_custNo_Event;
	var ib_offcCd_Event;
			
	//**********************************************************
	// 초기화
	//**********************************************************
	scwin.init = function() {
		gsSysDate 		= WebSquare.date.getCurrentServerDate("yyyyMMdd"); 	// global 현재일자
		gUserID 		= util.getGdsValue("gUserID");    					// global 사용자 ID
		gUserEmpName 	= util.getGdsValue("gUserEmpName");    				// global 부서명
						
	    //----- 버튼 처리(입력, 수정, 삭제, 결재, 취소, 인쇄, 엑셀, 닫기)
		gfn_btnEnable(false, false, false, false, false, false, false, true);
	        
	    ic_dateFr.setValue(gsSysDate);	//현재날짜 셋팅
	    ic_dateTo.setValue(gsSysDate);	//현재날짜 셋팅	    	    	   
	    
	    var maskRegExp = [ /^[A-Z\s]{1}[0-9\s]{4}[0-9\s]{7}$/,
	    						/^[0-9\s]{6}$/,
	    							/^[0-9\s]{6}[0-9\s]{1}[A-Z0-9\s]{6}$/
	    								,/^[0-9A-Z\s]{7}$/
	    									,/^[ㄱ-ㅎㅏ-ㅣ가-힣A-Za-z0-9\s]{20}$/ ];
		var maxLength  = [12,6,13,7,20];
		var chkIndex   = 0;	
		ib_custNo.setPlaceholder( "");
		ib_custNo_Event = new wMaskEditObj(ib_custNo, maskRegExp, maxLength, chkIndex);	
		ib_custNo.setPlaceholder("______-_______");
		ib_custNo.setDisplayFormat("######-#######");
		ib_custNo_Event.setIndex(2);		
		
		ib_offcCd.setPlaceholder( "");
		ib_offcCd_Event = new wMaskEditObj(ib_offcCd, maskRegExp, maxLength, chkIndex);
		ib_offcCd.setPlaceholder("_______");
		ib_offcCd.setDisplayFormat("#######");
		ib_offcCd_Event.setIndex(3);
	    
	        
	    var promise = Promise.resolve();
	    promise = promise.then(function(rtnVal) {
	     	// /common/CBSO0015.do 로그인자 권한조회	 
			scwin.getLginAuthInfo();	  			
	    });
	    
	    return promise;	
	    	
	};
	
	//**********************************************************
	// 
	//**********************************************************
	scwin.subCallBackfunction = function(e){
		// e.subID : 서브미션 ID
		// e.flag  : s성공;
		// e.msg   : 트렌젝션 처리후 메세지
		// e."받을데이타셋"
		switch(e.subID){
			// 로그인ID권한조회
			case "retrieveLginIDAuthInqr":
				if(e.flag == "s") {
					dst_LginAuthInqrVo.setJSON(e.result);	
					
					//슈퍼권한
					if(dst_LginAuthInqrVo.get("bizAdmrYn") == "Y"){
						ib_offcCd.setDisabled( false );								
					}else{
						ib_offcCd.setDisabled( true );
						ib_offcCd.setValue( dst_LginAuthInqrVo.get("pstnDeptCd"));
					}
					
					var promise = Promise.resolve();
					promise = promise.then(function(rtnVal) {
						// /sales/support/CBSB0018.do 모집인시험결과조회
   						scwin.getClprExamResltList();	
					});
					
				}else {
					wprom.alert(e.msg);
				}															
			break;
			
			// 모집인시험결과조회
			case "retrieveClprExamResltInqr":
				if(e.flag == "s") {
					dst_ClprExamResltInqrInfoListVo.setJSON(e.result.clprExamResltInqrInfoVo);
					
					if(dst_ClprExamResltInqrInfoListVo.getRowCount()==0){
			        	wprom.alert("조회결과가 없습니다.");        	
			        	return;	
					}
				}else {
					wprom.alert(e.msg);
				}															
			break;
		}
	};
	
	//**********************************************************
	// 로그인자 권한조회 /common/CBSO0015.do 로그인자 권한조회
	//**********************************************************
	scwin.getLginAuthInfo = function() {
		var promise = Promise.resolve();
		// 사번으로 로그인자 권한조회	
		//gUserID = "2080018";
		dst_LginAuthInqrVo.set("empNo" , gUserID);
		
		var trans = new transSub();                  	// transaction 보낼 객체 생성
		trans.setId("retrieveLginIDAuthInqr");        	// transaction 보낼 SubmissionID
		trans.setUrl("/common/CBSO0015.do");   			// Controller 쪽으로 보낼 주소
		trans.setDs("dst_LginAuthInqrVo");					
		trans.targetDs("dst_LginAuthInqrVo");		
		trans.callBack("scwin.subCallBackfunction");  	// 받을 Dataset을 처리시 콜백함수
		trans.send();
		
	};
	
//---------------------------------------------------------------------------
// function 명 : lfn_getRealNo (기간계B370)
// 내용        : 실주민번호 조회
//---------------------------------------------------------------------------
	scwin.lfn_getRealNo = function(sCustKeyNo) {
		DsWs_CustNo.removeAll();
		
		var MethodObj = new callService("com.inswave.longterm.com.com.task.AXR300Task", "selectRealCustNo", 0, 0);
		MethodObj.setId("selectRealCustNo");
		MethodObj.setParam("sCustKeyNo", sCustKeyNo);
		MethodObj.setUrl("/common/CBSO028MController.do");
		MethodObj.setXPath("//*/LTI0000");
		MethodObj.addXPath("//WARNING");
		MethodObj.sync();
		MethodObj.send();    

		DsWs_CustNo.setJSON(_tranSync.getDsJson("LTI0000"));
		
		if(DsWs_CustNo.getRowCount() > 0 && wconv.length(DsWs_CustNo.getCellData(0,"sRealCustNo")) >  0) {
			return DsWs_CustNo.getCellData(0,"sRealCustNo");
		}else{
			return sCustKeyNo;
		}
	};
	
//---------------------------------------------------------------------------
// function 명 : lfn_getCustNo
// 내용        : 고객번호 조회
//---------------------------------------------------------------------------
	scwin.lfn_getCustNo = function(sRealCustNo) {
		DsWs_CustNo.removeAll();		
		
		var MethodObj = new callService("com.inswave.longterm.com.com.task.AXR300Task", "selectCustKeyNo", 0, 0);
		MethodObj.setId("selectCustKeyNo");
		MethodObj.setParam("sRealCustNo", sRealCustNo);
		MethodObj.setUrl("/common/CBSO028MController.do");
		MethodObj.setXPath("//*/LTI0000");
		MethodObj.addXPath("//WARNING");
		MethodObj.sync();
		MethodObj.send();    

		DsWs_CustNo.setJSON(_tranSync.getDsJson("LTI0000"));
		
		if(DsWs_CustNo.getRowCount() > 0 && wconv.length(DsWs_CustNo.getCellData(0,"sCustKeyNo")) >  0) {
			return DsWs_CustNo.getCellData(0,"sCustKeyNo");
		}else{
			return "";
		}
	};
	
	//**********************************************************
	// 조회버튼 클릭시
	//**********************************************************
	scwin.btn_Search_onclick = function() {
	
		if(util.isNull(ic_dateFr.getValue())){			
        	wprom.alertF("조회 시작일자를 입력해 주세요.",{}, ic_dateFr);        	
        	return;		
		}		
		if(util.isNull(ic_dateTo.getValue())){			
        	wprom.alertF("조회 종료일자를 입력해 주세요.",{}, ic_dateTo);        	
        	return;		
		}				
		if(parseInt(ic_dateFr.getValue()-ic_dateTo.getValue())>0){
			wprom.alertF("조회시작일이 조회종료일보다 큽니다.",{},ic_dateFr);						
			return;
		}		
		if(WebSquare.date.dateDiff(ic_dateFr.getValue(),ic_dateTo.getValue())>31){
			wprom.alertF("최대 31일까지 조회 가능합니다.",{},ic_dateTo);
			return;
		}
		//모집인시험결과 list조회
		scwin.getClprExamResltList();
	};
	
