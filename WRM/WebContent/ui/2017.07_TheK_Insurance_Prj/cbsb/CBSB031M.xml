	// 모집인시험결과list조회 /sales/support/CBSB0018.do
	//**********************************************************
	scwin.getClprExamResltList = function() {
	
		//고객 주민번호 -> 치환키
		var custKey = "";
		if(!util.isNull(ib_custNo.getValue())){
