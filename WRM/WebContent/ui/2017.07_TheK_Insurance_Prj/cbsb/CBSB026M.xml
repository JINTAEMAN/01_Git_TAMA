		<script type="javascript"><![CDATA[
			
	var gsSysDate;			// global 현재일자
	var gUserID; 			// global 사용자 ID
	var gUserEmpName; 	    // global 부서명
	var ib_inputCustNo_Event;
			
	//**********************************************************
	// 초기화
	//**********************************************************
	scwin.init = function() {
		gsSysDate 		= WebSquare.date.getCurrentServerDate("yyyyMMdd"); 	// global 현재일자
		gUserID 		= util.getGdsValue("gUserID");    					// global 사용자 ID
		gUserEmpName 	= util.getGdsValue("gUserEmpName");    				// global 부서명
						
	    //----- 버튼 처리(입력, 수정, 삭제, 결재, 취소, 인쇄, 엑셀, 닫기)
		gfn_btnEnable(false, false, false, false, false, false, false, true);
		
	    var maskRegExp = [ /^[A-Z\s]{1}[0-9\s]{4}[0-9\s]{7}$/,
	    						/^[0-9\s]{6}$/,
	    							/^[0-9\s]{6}[0-9\s]{1}[A-Z0-9\s]{6}$/
	    								,/^[0-9A-Z\s]{7}$/
	    									,/^[ㄱ-ㅎㅏ-ㅣ가-힣A-Za-z0-9\s]{20}$/ ];
		var maxLength  = [12,6,13,7,20];
		var chkIndex   = 0;	
		ib_inputCustNo.setPlaceholder( "");
		ib_inputCustNo_Event = new wMaskEditObj(ib_inputCustNo, maskRegExp, maxLength, chkIndex);	
		ib_inputCustNo.setPlaceholder("______-_______");
		ib_inputCustNo.setDisplayFormat("######-#######");
		ib_inputCustNo_Event.setIndex(2);

		
	    var promise = Promise.resolve();
	    promise = promise.then(function(rtnVal) {
	     	// /common/CBSO0015.do 로그인자 권한조회	 
			scwin.getLginAuthInfo();	
  			
	    });	    
	    return promise;	
	};	
	
	
	//**********************************************************
	// 로그인자 권한조회 /common/CBSO005.do 로그인자 권한조회
	//**********************************************************
	scwin.getLginAuthInfo = function() {		
		// 사번으로 로그인자 권한조회	
		dst_LginAuthInqrVo.set("empNo" , gUserID);
		
		var trans = new transSub();                  	// transaction 보낼 객체 생성
		trans.setId("retrieveLginIDAuthInqr");        	// transaction 보낼 SubmissionID
		trans.setUrl("/common/CBSO0015.do");   			// Controller 쪽으로 보낼 주소
		trans.setDs("dst_LginAuthInqrVo");					
		trans.targetDs("dst_LginAuthInqrVo");		
		trans.callBack("scwin.subCallBackfunction");  	// 받을 Dataset을 처리시 콜백함수
		trans.send();
		
	};
	
	//**********************************************************
	// 
	//**********************************************************
	scwin.subCallBackfunction = function(e){
		// e.subID : 서브미션 ID
		// e.flag  : s성공;
		// e.msg   : 트렌젝션 처리후 메세지
		// e."받을데이타셋"
		switch(e.subID){
			// 로그인ID권한조회
			case "retrieveLginIDAuthInqr":
				if(e.flag == "s") {
		
					dst_LginAuthInqrVo.setJSON(e.result);		
					
					//장기업무부(4000103) - bizAdmrYn
					//대면총무(34024) - geafAdmrYn
					//모집인관리자(34025) - clprAdmrYn
					if(dst_LginAuthInqrVo.get("bizAdmrYn") == "Y"
						|| dst_LginAuthInqrVo.get("geafAdmrYn") == "Y"
						|| dst_LginAuthInqrVo.get("clprAdmrYn") == "Y"
					){												
						btn_Search.setDisabled(false);
							
						if(dst_LginAuthInqrVo.get("bizAdmrYn") == "Y"){
							btn_excelDown.setDisabled(true);
							btn_excelDown.visible(false);
						}
					}else{						
						btn_Search.setDisabled(true);
        				wprom.alert("화면조회권한이 없습니다.");
        				return;
					}
					
				}else {
					wprom.alert(e.msg);
					
				}															
			break;			
			
			//손보협회 전기관경력조회
			case "retrieveKniaTotOrgnCarerInfo":
				if(e.flag == "s") {
					dst_KniaOnline320InfoVo.setJSON(e.result);
					dst_knia320CarerInfoListVo.setJSON(e.result.knia320CarerInfoVo);
									
					if(dst_KniaOnline320InfoVo.get("errMsg")!= null
						&& dst_KniaOnline320InfoVo.get("errMsg")!= ""){
						alert(dst_KniaOnline320InfoVo.get("errMsg"));
					}
				}else{
					if(e.msg == null || e.msg == "null"){
						wprom.alert("손보협회와 통신이 원활하지 않습니다.\n잠시후에 다시 조회해주세요.");
					}else{
						wprom.alert(e.msg);
					}
				}
			break;
		}
	};
	
	
	//**********************************************************
	// 손보협회 모집인 전기관 경력조회
	//**********************************************************
	scwin.btn_Search_onclick = function() {
			
		wconv.clear(dst_knia320CarerInfoListVo);
	
		if(util.isNull(ib_inputCustNo.getValue())){			
        	wprom.alertF("주민등록번호를 입력해 주세요.",{},ib_inputCustNo);
        	return;
		}
		if(util.isNull(sel_psnInfoWrcsRvFlag.getValue())){			
        	wprom.alertF("조회목적을 선택해 주세요.",{},sel_psnInfoWrcsRvFlag);
        	return;
		}		
	
		dst_KniaOnline320InfoVo.set("custNo",ib_inputCustNo.getValue());
		dst_KniaOnline320InfoVo.set("psnInfoWrcsRvFlag",sel_psnInfoWrcsRvFlag.getValue());
		
		var trans = new transSub();                  	//transaction 보낼 객체 생성
		trans.setId("retrieveKniaTotOrgnCarerInfo");    //transaction 보낼 SubmissionID
		trans.setUrl("/sales/support/CBSB0028.do");   	//Controller 쪽으로 보낼 주소
		trans.setDs("dst_KniaOnline320InfoVo");					
		trans.targetDs("dst_KniaOnline320InfoVo");		
		trans.callBack("scwin.subCallBackfunction");  	//받을 Dataset을 처리시 콜백함수
		trans.send();
		
	};
	
    //**********************************************************
	// 손보협회 전기관 경력조회 엑셀 다운로드
	//**********************************************************	
	scwin.btn_excelDown_onclick = function() {
		var option={};
		option.fileName= "손보협회전기관경력조회_"+".xlsx";
	    common.downLoadExcel("grd_dataList",option,{});
	};
	]]></script>
