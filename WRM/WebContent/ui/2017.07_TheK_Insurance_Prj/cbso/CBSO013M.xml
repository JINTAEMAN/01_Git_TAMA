	scwin.sel_email_onviewchange = function(info) {
		var strCode = sel_email.getValue();		
		if(wconv.getLength(Ds_Main.get("sEmail")) == 0) {
        	sel_email.setSelectedIndex(0);
        	return;
	    }
	    
		if(wconv.getLength(strCode) == 0) {
        	return;
	    }
	    
	    var arrEmail = wconv.jsplit(Ds_Main.get("sEmail"), "@");
	    Ds_Main.set("sEmail", arrEmail[0] + strCode);		
