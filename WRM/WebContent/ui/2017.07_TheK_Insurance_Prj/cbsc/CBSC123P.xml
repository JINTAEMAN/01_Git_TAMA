	var columns = "S19";   
    var objects = "";
    var titleTypes =  "N"; 		// A:전체, S:선택, N:안넣
 
	gfn_getCommonCodes(dataSets, columns, objects, titleTypes);     
 
	wconv.copy(Ds_AgreRestTermCd, DsWs_AgreRestTermCd); 	  // 약정복구기간 Dataset Copy(Ds_ParkEqmtFlagCd 데이타 생성됨)  
 
