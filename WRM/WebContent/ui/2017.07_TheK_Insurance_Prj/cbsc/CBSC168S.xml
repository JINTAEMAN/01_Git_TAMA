 //---------------------------------------------------------------------------
// function 명 : body1_onpageload
// 내용        : 화면로딩시 처리
//---------------------------------------------------------------------------
scwin.init = function() {  
	//console.log("[/CBSC168S.xml] [init()] ==> [폼 로딩] [운전자고지사항[B230_Div3_Pop]] [TEST_01] ===========> @@" );
 	
    if (sScnNm == "E110") {
        var sStatic33 = Static33.getValue();
        
        sStatic33 = Static33 + "\n\u203B 부활(효력회복)시에는 계약전 알릴의무 대상 기간을\u300C최초 계약해당일(또는 직전 부활(효력회복)일) 이후로부터 부활(효력회복)을 청약한 날까지의 기간\u300D과 \u300C계약전 알릴의무 대상기간(아래질문의 최근 1년, 5년)\u300D중 짧은 기간으로 합니다.";
        Static33.setValue(sStatic33);
    }
 
  	CBSC129P.scwin.lfn_Search();   // 피보험자고지사항[B230_Pop] (/CBSC129P.xml)에서 조회
 
  	CBSC129P.scwin.lfn_ComponentsSort();	// 피보험자고지사항[B230_Pop] (/CBSC129P.xml)에서 Components 정렬시킴
 
	console.log("[/CBSC168S.xml] [init()] ==> [폼 로딩] [운전자고지사항[B230_Div3_Pop]] [TEST_end] ========================> @@@@  \n\n\n" ); 
}; 
 
 //---------------------------------------------------------------------------
// function 명 : lfn_RadioOnChanged
// 내용        : 예/아니오 선택시 아니오일 경우 연관객체 초기화 시킴
//--------------------------------------------------------------------------- 
lfn_RadioOnChanged = function(info) {
	//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_01] [this.getID()] "+ this.getID() );
 
    var comp;
    var arrList;
    var arrListTmp;  
	arrList = this.getID().split("_");	 // split 처리
    var compCnt = B230_Div1.getChildrenCount();		// Components 총 수 
  	//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_02] [this.getID()] "+ this.getID() +"[this.getValue()]"+ this.getValue() +"[compCnt]"+ compCnt ); 
    
    for (i = 0; i < compCnt; i++) {	 //정렬할 배열 담기 
        comp = B230_Div1.getAllChildren()[i];		// Components 담기 
		//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_02_1] [i_번째]"+ i +"[comp.id]"+ comp.id );
        
   		if (util.isNull(comp)) {		// comp가 널 이면 다음 i번째로 진행
        	continue;
         }
 		if (comp.id.indexOf("Static") > -1 || comp.id.indexOf("static") > -1) {	     // static 존재하면(제외 시킴) 
 			 continue;
        }   
        if (comp.id.indexOf("group") > -1) {	     // group 존재하면(제외 시킴) ;
 	  		continue;
        }          
		if (comp.id.indexOf("Shape") > -1) {	     // Shape 존재하면(제외 시킴)  
 	  		continue;
        }         
        if (comp.id.indexOf("wq_uuid") > -1) {	     // wq_uuid 존재하면(제외 시킴)  
 	  		continue;
        }  
    	var comp2 = {};  
    	comp2.id = comp.id;	// Component ID 
     	var tmpObj = WebSquare.util.getComponentById(comp2.id);   
     	comp2.getType = tmpObj.getPluginName();		// Component 타입
    	comp2.value = tmpObj.getValue();			// Component value  
    	comp2.enable = !tmpObj.getDisabled();	 		// Component disabled  Obj.getValue()
    	comp2.Obj   = tmpObj; 
       	//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_12] [i_번째]"+ i +"[comp2.id]"+ comp2.id +"[comp2.Obj.getValue()]"+ comp2.Obj.getValue() +"[parent.sDrveType]"+ parent.sDrveType );
               
        if (comp2.id.indexOf(parent.sDrveType) > 0) {	   // 같은 sDrveType이 존재하면
        	//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_M_21] [i_번째]"+ i +"[comp2.id]"+ comp2.id +"[arrList[1]]"+ arrList[1] +"[substr(arrList[1], 0, 4)]"+ wconv.substr(arrList[1], 0, 4)  +"[comp2.id.indexOf(wconv.substr(arrList[1], 0, 4))]"+ comp2.id.indexOf(wconv.substr(arrList[1], 0, 4)) );
   	    
           if (comp2.id.indexOf(wconv.substr(arrList[1], 0, 4)) > 0) {	 // 4자리 같은 질의코드가 존재하면
            	arrListTmp = comp2.id.split("_");	 // split 처리
          		//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_M_51] [i_번째]"+ i +"[comp2.id]"+ comp2.id +"[arrList[1]]"+ arrList[1] +"[this.getID()]"+ this.getID() +"[this.getValue()]"+ this.getValue() );
   			     			 	  
				if (CBSC129P.sSvtYn != "Y" && arrListTmp[1] == "B0703") {	 	// 저축보험용은 비활성화 
	   				//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_32] [비활성화 처리] [i_번째]"+ i );
	   			 
	      			comp2.Obj.setValue("");   // 해당 컴포넌트와 관련된 하위 컴포넌트 널 처리  
	           		//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_92] [비활성화 처리] [i_번째]"+ i );
	           		comp2.enable = false;
	           		comp2.Obj.setDisabled(true);		// 비활성화 처리 
					comp2.Obj.setReadOnly(true);		// 비 활성화 처리(ReadOnly)
	            	//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_97] [비활성화 처리] [i_번째]"+ i +"[comp2.id]"+ comp2.id +"[this.getID()] "+ this.getID() +"[this.getValue()]"+ this.getValue() );
	   		 	 }  // end of if ==> 저축보험용은 비활성화  
				 
				if (wconv.substr(arrListTmp[1], 4, 1) != "0" && arrListTmp[1] != "B0600") {    //예/아니오 객체는 빼고 나머지만 초기화 
					if (this.getValue() == "N") {		//  아니오 이면   
               	 		//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_91] [비활성화 처리] [i_번째]"+ i +"[comp2.id]"+ comp2.id +"[comp2.getType]"+ comp2.getType +"[this.getValue()]"+ this.getValue() ); 
						comp2.Obj.setValue("");   // 해당 컴포넌트와 관련된 하위 컴포넌트 널 처리  
						comp2.enable = false;
						comp2.Obj.setDisabled(true);		// 비활성화 처리 
						//comp2.Obj.setReadOnly(true);		// 비 활성화 처리(ReadOnly)
						//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_91_2] [비활성화 처리] [i_번째]"+ i +"[comp2.id]"+ comp2.id +"[this.getID()] "+ this.getID() +"[this.getValue()]"+ this.getValue() );
					 } else { 
						comp2.enable = true;
						comp2.Obj.setDisabled(false);		// 활성화 처리
						comp2.Obj.setReadOnly(false);	// 활성화 처리(ReadOnly)
					} 
                }
                //console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_M_52] [i_번째]"+ i +"[comp2.id]"+ comp2.id +"[arrList[1]]"+ arrList[1] +"[this.getID()] "+ this.getID() );
   			 
            }  //  end of if ==> 4자리 같은 질의코드가 존재하면
            
		}  //  end of if ==> 같은 sDrveType이 존재하면
		//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_12_end] [같은 질의코드를 찾음][i_번째]"+ i +"[comp2.id]"+ comp2.id +"[comp2.Obj.getValue()]"+ comp2.Obj.getValue() +"[parent.sDrveType]"+ parent.sDrveType );
	
	}	//  end of for() 
    
	//console.log("[/CBSC168S.xml] [lfn_RadioOnChanged()] ==> [Radio 변경 이벤트 처리] [TEST_end]" );
};
]]></script>
