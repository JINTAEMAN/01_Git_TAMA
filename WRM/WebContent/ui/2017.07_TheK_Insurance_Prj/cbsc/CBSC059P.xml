};

//---------------------------------------------------------------------------
// function 명 : lfn_clearDisplay
// 내용        : 화면초기화
//---------------------------------------------------------------------------
scwin.lfn_clearDisplay = function() {
    // 계약번호 유무에 따른 화면처리
    if (wconv.trim(sCrNo) == "" || wconv.trim(sCrNo) == null) {
        msk_sCrNo.setDisabled(false);
        btn_search.show();
    } else {
        // datamove
        msk_sCrNo.setValue(sCrNo.toUpperCase());
        edt_sCrtor.setValue(sCrtor);
        edt_sCrStat.setValue(sCrStat);
        edt_sInsurTerm.setValue(sInsurTerm);
