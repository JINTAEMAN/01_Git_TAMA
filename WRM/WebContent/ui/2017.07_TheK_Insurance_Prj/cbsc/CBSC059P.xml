var sCrtor = "";
// 계약상태
var sCrStat = "";
// 보험기간
var sInsurTerm = "";

//---------------------------------------------------------------------------
// function 명 : D050_OnLoadCompleted
// 내용        : OnLoadCompleted
//---------------------------------------------------------------------------
scwin.init = function() {
	wprom.windowOpenInit(D050_Pop);
	
	var inJson; 
	var indata = $w.getParameter("param");
	inJson = JSON.parse(indata);
	try {
		sCrNo      = inJson.sCrNo;
		sCrtor     = inJson.sCrtor; 
		sCrStat    = inJson.sCrStat;
		sInsurTerm = inJson.sInsurTerm;
	} catch(e) {}
	
    // 화면 초기화
    scwin.lfn_clearDisplay();
};

//---------------------------------------------------------------------------
// function 명 : lfn_Search
// 내용        : 입출금거래내역조회
//---------------------------------------------------------------------------
//비동기호출 
scwin.lfn_Search_Async = function() {
	util.eventAsyncCall(this, scwin.lfn_Search, arguments);
};

scwin.lfn_Search = function(obj) {
	wconv.clear(Ds_LTIFA01);
    wconv.clear(DsWs_LTIFA01);
    wconv.clear(warning);
	
	msk_nPremSum.setValue(0);
    var sCrNo = msk_sCrNo.getValue();

    // 입력값 chk
    if (!scwin.lfn_inputChk()) {
        return;
    }

	var transObj = new callService("com.inswave.longterm.psv.ens.task.EAD050Task", "getPremRecvWdrcDlSpec", "", "");
	transObj.setUrl("/contract/CBSC059PController.do");
	transObj.setId("selectCntrDtl");
	transObj.setParam("sCrNo", msk_sCrNo.getValue()); // 계약번호
	transObj.setXPath("//*/vector/data/LTIFA01");
	transObj.addXPath("//*/WARNING");
	//transObj.callBack("scwin.subCallBackfunction");
	transObj.sync();
	transObj.send();
	//util.interfaceKey(_tranSync.result.serviceDs);
	
	util.jSonDsCreate(_tranSync.getDsJson("WARNING"), "warning");
	if (warning.getRowCount() != 0) {
        wprom.alert(warning.getCellData(0, "msg"));
        return;
    }
    
    Ds_LTIFA01.setJSON(_tranSync.getDsJson("LTIFA01"));
    DsWs_LTIFA01.setJSON(_tranSync.getDsJson("LTIFA01"));
	
    //Ds_LTIFA01.Sort("nDlSeqno", false);
    //Ds_LTIFA01.Sort("nPaymSeq", false);
    msk_nPremSum.setValue(Ds_LTIFA01.getColSum("nRealRecpPrem"));
};

//---------------------------------------------------------------------------
// function 명 : lfn_inputChk
// 내용        : 입력값체크(조회시)
//---------------------------------------------------------------------------
scwin.lfn_inputChk = function(obj) {
    // 계약자코드
    if (msk_sCrNo.getValue() == "") {
        wprom.alert("계약번호를 입력하세요.");
        return false;
    }
    return true;
};

